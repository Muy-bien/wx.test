<import src="../stars/stars_template.wxml" />
<template name="movieTemplate">
  <view class='movie_container' catchtap='onMovieTap' data-movieId="{{movieId}}">
    <image class='movie_img' src="{{coverageUrl}}"></image>
    <text class='movie_title'>{{title}}</text>
    <template is="starsTemplate" data="{{average:average,stars:stars}}" />
  </view>
</template>